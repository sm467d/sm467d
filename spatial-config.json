{
  "meta": {
    "name": "Spatial Computing Development Environment",
    "version": "2.1.0",
    "author": "sm467d",
    "description": "Modern aesthetics meets spatial computing excellence",
    "license": "MIT"
  },
  "theme": {
    "primary": {
      "neural": "#00D4FF",
      "quantum": "#8B5CF6",
      "void": "#0A0A0A",
      "hologram": "#39FF14",
      "plasma": "#FF6B35"
    },
    "gradients": {
      "spatial": "linear-gradient(135deg, #667eea 0%, #764ba2 100%)",
      "neural": "linear-gradient(45deg, #00D4FF 0%, #8B5CF6 50%, #FF6B35 100%)",
      "quantum": "radial-gradient(circle at 50% 50%, #8B5CF6 0%, #1A1A2E 100%)",
      "holographic": "conic-gradient(from 0deg, #00D4FF, #8B5CF6, #39FF14, #FF6B35, #00D4FF)"
    },
    "typography": {
      "primary": "Inter Variable",
      "mono": "JetBrains Mono",
      "display": "Poppins",
      "weights": [300, 400, 500, 600, 700]
    },
    "spacing": {
      "unit": "8px",
      "grid": "24px",
      "golden": "1.618"
    }
  },
  "spatial": {
    "dimensions": {
      "viewport": {
        "width": 1920,
        "height": 1080,
        "depth": 512
      },
      "workspace": {
        "x": 3840,
        "y": 2160,
        "z": 1024
      },
      "interaction": {
        "near": 0.1,
        "far": 1000,
        "comfort": 2.5
      }
    },
    "physics": {
      "gravity": -9.81,
      "friction": 0.8,
      "restitution": 0.3,
      "timeScale": 1.0
    },
    "rendering": {
      "quality": "ultra",
      "shadows": "realtime",
      "antialiasing": "msaa-8x",
      "postprocessing": ["bloom", "ssr", "ambient-occlusion", "motion-blur"]
    },
    "haptics": {
      "enabled": true,
      "strength": 0.7,
      "frequency": 60,
      "patterns": ["click", "hover", "drag", "drop", "error", "success"]
    }
  },
  "workflow": {
    "environments": {
      "development": {
        "name": "Neural Sandbox",
        "color": "#00D4FF",
        "tools": ["vscode", "browser", "terminal", "simulator"],
        "layout": "floating-panels"
      },
      "design": {
        "name": "Quantum Canvas",
        "color": "#8B5CF6",
        "tools": ["figma", "blender", "photoshop", "substance"],
        "layout": "spatial-grid"
      },
      "collaboration": {
        "name": "Holographic Hub",
        "color": "#39FF14",
        "tools": ["slack", "notion", "miro", "zoom"],
        "layout": "circular-meeting"
      }
    },
    "gestures": {
      "pinch": "zoom",
      "swipe-left": "prev-workspace",
      "swipe-right": "next-workspace",
      "double-tap": "focus-mode",
      "palm-up": "command-palette",
      "fist": "grab-mode"
    },
    "automation": {
      "ai-assistant": {
        "enabled": true,
        "model": "claude-3-5-sonnet",
        "context": "spatial-computing",
        "voice": "natural"
      },
      "code-completion": {
        "provider": "github-copilot",
        "spatial-aware": true,
        "context-radius": 50
      }
    }
  },
  "performance": {
    "optimization": {
      "level-of-detail": true,
      "frustum-culling": true,
      "occlusion-culling": true,
      "batch-rendering": true,
      "memory-pooling": true
    },
    "metrics": {
      "target-fps": 90,
      "max-latency": 20,
      "render-budget": 11.1,
      "memory-limit": "8GB"
    },
    "adaptive": {
      "quality-scaling": true,
      "resolution-scaling": true,
      "effect-scaling": true,
      "fallback-2d": true
    }
  },
  "accessibility": {
    "vision": {
      "contrast-ratio": 4.5,
      "color-blind-support": true,
      "font-scaling": true,
      "motion-reduction": "respect-preference"
    },
    "motor": {
      "gesture-alternatives": true,
      "dwell-time": 500,
      "voice-control": true,
      "eye-tracking": true
    },
    "cognitive": {
      "simplified-ui": false,
      "focus-indicators": true,
      "progress-feedback": true,
      "error-recovery": true
    }
  },
  "integrations": {
    "vr": {
      "platforms": ["quest", "vive", "index", "pico"],
      "hand-tracking": true,
      "eye-tracking": true,
      "passthrough": true
    },
    "ar": {
      "platforms": ["hololens", "magic-leap", "arkit", "arcore"],
      "occlusion": true,
      "plane-detection": true,
      "object-tracking": true
    },
    "web": {
      "webxr": true,
      "webgl": "2.0",
      "wasm": true,
      "pwa": true
    }
  },
  "security": {
    "encryption": "aes-256",
    "auth": "oauth2",
    "permissions": "granular",
    "audit": true,
    "sandbox": true
  },
  "experimental": {
    "neural-interface": {
      "enabled": false,
      "protocols": ["bci", "eeg", "fmri"],
      "safety": "triple-redundant"
    },
    "quantum-computing": {
      "enabled": false,
      "simulator": "qiskit",
      "error-correction": true
    },
    "ai-vision": {
      "enabled": true,
      "models": ["yolo", "segment-anything", "depth-estimation"],
      "real-time": true
    }
  },
  "shortcuts": {
    "global": {
      "⌘+Space": "command-palette",
      "⌘+⇧+P": "spatial-search",
      "⌘+T": "teleport-mode",
      "⌘+G": "gesture-mode",
      "⌘+H": "hologram-toggle",
      "⌘+⇧+D": "debug-overlay"
    },
    "spatial": {
      "grip+trigger": "grab-object",
      "pinch": "precision-select",
      "point+click": "ray-interact",
      "palm-face": "UI-summon",
      "thumb-up": "confirm",
      "peace-sign": "screenshot"
    }
  },
  "assets": {
    "models": {
      "format": "gltf2",
      "compression": "draco",
      "optimization": "auto-lod"
    },
    "textures": {
      "format": "basis-universal",
      "compression": "astc",
      "mipmaps": true
    },
    "audio": {
      "format": "opus",
      "spatial": "hrtf",
      "compression": "adaptive"
    }
  }
}